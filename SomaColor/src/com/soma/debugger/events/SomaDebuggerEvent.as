package com.soma.debugger.events {	import flash.events.Event;	/**	 * <b>Author:</b> Romuald Quantin - <a href="http://www.soundstep.com/" target="_blank">www.soundstep.com</a><br />	 * <b>Class version:</b> 1.0<br />	 * <b>Actionscript version:</b> 3.0<br />	 * <b>Copyright:</b> 	 * <br />	 * <b>Date:</b> Sep 21, 2009<br />	 * <b>Usage:</b>	 * @example	 * <listing version="3.0"></listing>	 */		public class SomaDebuggerEvent extends Event {		//------------------------------------		// private, protected properties		//------------------------------------				//------------------------------------		// public properties		//------------------------------------				public static const SHOW_DEBUGGER:String = "Soma::SomaDebuggerEvent.SHOW_DEBUGGER";		public static const HIDE_DEBUGGER:String = "Soma::SomaDebuggerEvent.HIDE_DEBUGGER";		public static const MOVE_TO_TOP:String = "Soma::SomaDebuggerEvent.MOVE_TO_TOP";		public static const PRINT:String = "Soma::SomaDebuggerEvent.PRINT";		public static const CLEAR:String = "Soma::SomaDebuggerEvent.CLEAR";				public var message:Object;		public var debuggerName:String;		//------------------------------------		// constructor		//------------------------------------				public function SomaDebuggerEvent(type:String, message:Object = null, debuggerName:String = null, bubbles:Boolean = true, cancelable:Boolean = false) {			this.message = message;			this.debuggerName = debuggerName;			super(type, bubbles, cancelable);		}				//		// PRIVATE, PROTECTED		//________________________________________________________________________________________________				//		// PUBLIC		//________________________________________________________________________________________________				override public function clone():Event {			return new SomaDebuggerEvent(type, message, debuggerName, bubbles, cancelable);		}				override public function toString():String {			return formatToString("SomaDebuggerEvent", "message", "debuggerName", "type", "bubbles", "cancelable", "eventPhase");		}			}}